<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no">
  <title>DS3: Board of GCP</title>
  <link rel="stylesheet" href="/style.css">
</head>
<body>
  <nav>
    <div class="wrapper">
      <a href="/logout">Logout</a> <a href="/dashboard">Dashboard</a>
    </div>
  </nav>
  <div id="main">
    <div class="wrapper">
      <div class="output_content">
        <ul class="output_list">
          <% foreach my $label (keys %$commands) { %>
          <li>
            <form action="" method="post">
              <div class="text"><%= $label %></div>
              <input type="hidden" value="<%= $label %>" name="command">
              <button type="submit" class="btn">Go</button>
            </form>
          </li>
          <% } %>
        </ul>
        <div class="output_demo">
          <pre>
<%= $cmd_label %>
<%= $command %>

<%= $stdout %>
          </pre>
        </div>
      </div>
    </div>
  </div>
</body>
</html>